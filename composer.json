{
    "name": "laravel-turbo/jetstream-turbo",
    "description": "A feature turbo boost for Laravel Jetstream",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/jetstream/issues",
        "source": "https://github.com/laravel/jetstream"
    },
    "authors": [
        {
            "name": "Ian Tasker",
            "email": "ianwtasker@outlook.com"
        }
    ],
    "require": {
        "filament/filament": "^1.9",
        "illuminate/support": "^8.0",
        "laravel/jetstream": "^2.0",
        "livewire/livewire": "^2.3.5",
        "spatie/laravel-sluggable": "^3.1",
        "stechstudio/filament-impersonate": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "inertiajs/inertia-laravel": "^0.3",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelTurbo\\JetstreamTurbo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\JetstreamTurbo\\Tests\\": "tests/",
            "App\\": "stubs/app/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "LaravelTurbo\\JetstreamTurbo\\JetstreamTurboServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
